[options]
allow_bold = false
#audible_bell = false
clickable_url = true
dynamic_title = true
font = Hack Bold 9.8
#fullscreen = true
#geometry = 640x480
icon_name = terminal
mouse_autohide = true
#scroll_on_output = false
#scroll_on_keystroke = true

# Length of the scrollback buffer, 0 disabled the scrollback buffer
# and setting it to a negative value means "infinite scrollback"
scrollback_lines = 5000

#search_wrap = true
#urgent_on_bell = true
#hyperlinks = false

# $BROWSER is used by default if set, with xdg-open as a fallback
#browser = xdg-open

# "system", "on" or "off"
#cursor_blink = system

# "block", "underline" or "ibeam"
#cursor_shape = block

# Hide links that are no longer valid in url select overlay mode
#filter_unmatched_urls = true

# Emit escape sequences for extra modified keys
#modify_other_keys = false

# set size hints for the window
#size_hints = false

# "off", "left" or "right"
scrollbar = off

[colors]
# Base16 Material Lighter
# Author: Nate Peterson

foreground          = #80cbc4
foreground_bold     = #80cbc4
cursor              = #80cbc4
cursor_foreground   = #fafafa
background          = rgba(250, 250, 250, 1)

# 16 color space

# Black, Gray, Silver, White
color0  = #fafafa
color8  = #ccd7da
color7  = #80cbc4
color15 = #eeffff

# Red
color1  = #ff5370
color9  = #ff5370

# Green
color2  = #91b859
color10 = #91b859

# Yellow
color3  = #ffb62c
color11 = #ffb62c

# Blue
color4  = #6182b8
color12 = #6182b8

# Purple
color5  = #7c4dff
color13 = #7c4dff

# Teal
color6  = #39adb5
color14 = #39adb5

# Extra colors
color16 = #f76d47
color17 = #e53935
color18 = #e7eaec
color19 = #cceae7
color20 = #8796b0
color21 = #80cbc4

[hints]
#font = Monospace 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

